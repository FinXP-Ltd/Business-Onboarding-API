{
    "name": "finxp/businesses-onboarding-api",
    "type": "project",
    "description": "Businesses Onboarding API",
    "keywords": [
        "framework",
        "laravel"
    ],
    "authors": [
        {
            "name": "Joy Siriban",
            "email": "joy.siriban@finxp.com"
        },
        {
            "name": "Robert Real",
            "email": "robert.real@finxp.com"
        },
        {
            "name": "Raven Cabrera",
            "email": "raven.cabrera@finxp.com"
        },
        {
            "name": "Bryan Reyes",
            "email": "bryan.reyes@finxp.com"
        },
        {
            "name": "Marjorie Asensi",
            "email": "marjorie.asensi@finxp.com"
        }
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:FinXP-Ltd/APILogger.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:FinXP-Ltd/Reactor.git"
        }
    ],
    "require": {
        "php": "^8.2.20",
        "archtechx/enums": "^1.0.1",
        "auth0/login": "^7.12.0",
        "doctrine/dbal": "^3.5.1",
        "finxp/api-logger": "^v1.0.2-beta",
        "finxp/reactor": "^0.1.4@beta",
        "guzzlehttp/guzzle": "^7.8.1",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.9",
        "league/iso3166": "^4.3",
        "predis/predis": "^2.2",
        "sentry/sentry-laravel": "^4.3",
        "spatie/laravel-permission": "^6.3",
        "tucker-eric/eloquentfilter": "^3.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.13.11",
        "laravel/sail": "^1.27",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.6",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "vendor/bin/phpunit --configuration phpunit.xml"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
